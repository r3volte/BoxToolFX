                    <font>
                        <Font name="Bauhaus 93" size="30.0"/>
                    </font>
                </JFXButton>
                <FontAwesomeIconView fill="#da1a1a" glyphName="CHECK_CIRCLE_ALT" layoutX="460.0" layoutY="81.0"
                                     size="30"/>
                <FontAwesomeIconView fill="#da1a1a" glyphName="TIMES_CIRCLE" layoutX="462.0" layoutY="173.0" size="30"/>
