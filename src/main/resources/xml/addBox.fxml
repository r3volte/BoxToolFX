<?xml version="1.0" encoding="UTF-8"?>

<?import com.jfoenix.controls.JFXButton?>
<?import de.jensd.fx.glyphs.fontawesome.FontAwesomeIconView?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.StackPane?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>

<StackPane fx:id="rootPane" minHeight="250.0" minWidth="450.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1">
    <children>
        <AnchorPane maxHeight="-Infinity" maxWidth="-Infinity" minHeight="250.0" minWidth="450.0" prefHeight="250.0" prefWidth="520.0" style="-fx-background-color: #1e213d;">
            <children>
                <TextField fx:id="numBox" layoutX="109.0" layoutY="54.0" promptText="%enternumber">
                    <font>
                        <Font name="System Italic" size="12.0" />
                    </font>
                </TextField>
                <TextField fx:id="boxHeight" layoutX="109.0" layoutY="89.0" promptText="%enterheight">
                    <font>
                        <Font name="System Italic" size="12.0" />
                    </font>
                </TextField>
                <TextField fx:id="boxWidth" layoutX="109.0" layoutY="124.0" promptText="%enterwidth" />
                <JFXButton fx:id="submitBoxButton" layoutX="358.0" layoutY="26.0" prefHeight="82.0" prefWidth="148.0" text="%submit" textFill="#fcfaf5">
                    <font>
                        <Font name="Bitstream Vera Sans Bold" size="25.0" />
                    </font>
                </JFXButton>
                <JFXButton fx:id="clearBoxButton" layoutX="349.0" layoutY="118.0" prefHeight="82.0" prefWidth="148.0" text="%clear" textFill="#fcfaf5">
                    <font>
                        <Font name="Bitstream Vera Sans Bold" size="25.0" />
                    </font>
                </JFXButton>
                <FontAwesomeIconView fill="#fcfaf5" glyphName="CHECK_CIRCLE_ALT" layoutX="328.0" layoutY="77.0" size="30" />
                <FontAwesomeIconView fill="#fcfaf5" glyphName="TIMES_CIRCLE" layoutX="330.0" layoutY="169.0" size="30" />
            <Text fill="#fcfaf5" layoutX="32.0" layoutY="71.0" strokeType="OUTSIDE" strokeWidth="0.0" text="%number">
               <font>
                  <Font name="Bitstream Vera Sans Bold" size="14.0" />
               </font>
            </Text>
            <Text fill="#fcfaf5" layoutX="32.0" layoutY="106.0" strokeType="OUTSIDE" strokeWidth="0.0" text="%height">
               <font>
                  <Font name="Bitstream Vera Sans Bold" size="14.0" />
               </font>
            </Text>
            <Text fill="#fcfaf5" layoutX="32.0" layoutY="141.0" strokeType="OUTSIDE" strokeWidth="0.0" text="%width">
               <font>
                  <Font name="Bitstream Vera Sans Bold" size="14.0" />
               </font>
            </Text>
            </children>
            <padding>
                <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
            </padding>
        </AnchorPane>
    </children>
</StackPane>
