                <ChoiceBox layoutX="167.0" layoutY="104.0" prefWidth="100.0"/>
                <Label layoutX="298.0" layoutY="109.0" text="PCS" textFill="#da1a1a">
                    <font>
                        <Font name="Bauhaus 93" size="13.0"/>
                    </font>
                </Label>
                <Label layoutX="39.0" layoutY="143.0" text="Coated confguration" textFill="#da1a1a">
                    <font>
                        <Font name="Bauhaus 93" size="13.0"/>
                    </font>
                </Label>
                <ChoiceBox layoutX="167.0" layoutY="139.0" prefWidth="100.0"/>
                <Label layoutX="41.0" layoutY="179.0" text="Drums configuration" textFill="#da1a1a">
                    <font>
                        <Font name="Bauhaus 93" size="13.0"/>
                    </font>
                </Label>
                <ChoiceBox layoutX="167.0" layoutY="174.0" prefWidth="100.0"/>
                <ChoiceBox fx:id="discPcs" layoutX="328.0" layoutY="104.0" prefWidth="50.0"/>
